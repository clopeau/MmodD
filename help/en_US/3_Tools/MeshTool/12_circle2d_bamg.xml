  <refnamediv>
    <refname>circle2d_bamg</refname><refpurpose>Create a circle mesh</refpurpose>
  </refnamediv>



<refsynopsisdiv>
   <title>Calling Sequence</title>
   <synopsis>
   circle2d_bamg(R,n)
   </synopsis>
</refsynopsisdiv>

<refsection>
   <title>Parameters</title>
   <variablelist>
   <varlistentry><term>R</term>
      <listitem><para>Radius</para></listitem></varlistentry>
<varlistentry><term>n</term>
      <listitem><para>number of equidistributed points on the boundary</para></listitem></varlistentry>

   </variablelist>
</refsection>


<refsection>
   <title>Examples</title>
   <programlisting role="example"><![CDATA[
  th=circle2d_bamg(1,100)
  meshtool(th)
   ]]></programlisting>
</refsection>

